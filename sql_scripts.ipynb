    "import sqlite3\n",
    "import pandas as pd\n",
    "\n",
    "conn = sqlite3.connect('ecommerce.db')\n",
    "\n",
    "df = pd.read_sql_query(\"\"\"\n",
    "    SELECT user_id, signup_date, country\n",
    "    FROM users\n",
    "    LIMIT 5\n",
    "\"\"\", conn)\n",
    "\n",
    "df\n"
